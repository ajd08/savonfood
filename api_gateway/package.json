{
    "name": "api_gateway",
    "version": "1.0.0",
    "description": "api gateway for savonfood",
    "main": "dist/index.js",
    "scripts": {
        "prebuild": "",
        "build": "tsc",
        "prestart": "npm run build",
        "start": "nodemon ./src/index.ts",
        "test": "npx jest --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ajd08/savonfood.git"
    },
    "author": "Anthony",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ajd08/savonfood/issues"
    },
    "homepage": "https://github.com/ajd08/savonfood#readme",
    "dependencies": {
        "axios": "^0.21.1",
        "express": "^4.17.1",
        "puppeteer": "^8.0.0",
        "recipe-ingredient-parser-v2": "^1.1.1",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.33",
        "jest": "^26.6.3",
        "ts-jest": "^26.5.3",
        "tslint": "^6.1.3",
        "typescript": "^4.2.3"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ext": "ts",
        "ignore": [
            "*.test.ts"
        ],
        "delay": "3",
        "execMap": {
            "ts": "ts-node"
        }
    }
}
